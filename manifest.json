{
  "manifest_version": 2,
  "name": "Slack - rename",
  "description": "This extension help you to toogle an information next to your name",
  "version": "1.2",
  "background": { "scripts": ["background.js"] },
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "slack-rename.html",
    "default_title": "Click to toogle _off"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://*.slack.com/account/settings"
  ]
}